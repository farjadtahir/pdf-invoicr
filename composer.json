{
    "name": "luisinder/pdf-invoice",
    "description": "Library to generate PDF invoices",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "keywords": ["invoice", "pdf", "konekt", "artkonekt"],
    "support": {
        "source": "https://github.com/luisinder/pdf-invoice"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/luisinder"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "setasign/fpdf": "~1.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": { "Konekt\\PdfInvoice\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\PdfInvoice\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
